<form action="{{ path('recruiter_recruit_edit', {id : recruit.id}) }}" method="post" {{ form_enctype(form) }}>
	<label for="user_first_name">First Name</label>
	<input value="{{ recruit.user.firstname }}" type="text" id="user_first_name" name="user[first_name]" />
	<label for="user_last_name">Last name</label>
	<input value="{{ recruit.user.lastname }}" type="text" id="user_last_name" name="user[last_name]" />
    {{ form_widget(form) }}
</form>